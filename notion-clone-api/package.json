{
  "name": "notion-clone-api",
  "version": "1.0.0",
  "description": "A minimalistic wiki and notes app for organizing content",
  "main": "notion-clone-api.js",
  "scripts": {
    "start": "node notion-clone-api.js",
    "dev": "nodemon notion-clone-api.js",
    "client": "cd frontend && npm start",
    "client-install": "cd frontend && npm install",
    "client-build": "cd frontend && npm run build",
    "setup": "npm install && npm run client-install && npm run client-build",
    "dev-full": "concurrently \"npm run dev\" \"npm run client\""
  },
  "private": true,
  "keywords": [
    "notion",
    "wiki",
    "notes",
    "api"
  ],
  "author": "Notion Clone Team",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "mariadb": "^3.2.3",
    "swagger-ui-express": "^5.0.0",
    "yamljs": "^0.3.0"
  },
  "devDependencies": {
    "concurrently": "^7.6.0",
    "nodemon": "^3.0.2"
  }
}
